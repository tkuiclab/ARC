<?xml version="1.0" ?>
<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find iclab7a_description)/config/position_controller.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
	output="screen" ns="/robotis_pro7a" args="joint_state_controller
																						joint1_position
																						joint2_position
																						joint3_position
																						joint4_position
																						joint5_position
																						joint6_position"/>

</launch>
